sh -c "$(curl -fsSL https://raw.github.com/robbyrussell/oh-my-zsh/master/tools/install.sh)"
wget https://www.python.org/ftp/python/3.7.10/Python-2.7.10.tar.xz
tar -xvf Python-2.7.10.tar.xz
cd Python-2.7.10
./configure --prefix=/home/mfrancis/python
make install
virtualenv p27 --python=/home/mfrancis/python/bin/python
source p27/bin/activate
pip install readline
pip install pandas
pip install seaborn

